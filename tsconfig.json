{
  "compilerOptions": {
    "target": "es2016",
    "lib": [
      "esnext",
      "DOM"
    ],
    "baseUrl": ".",
    "rootDir": ".",
    "module": "esnext",
    "moduleResolution": "node",
    "types": [
      "jest"
    ],
    "strict": true,
    "noImplicitAny": false,
    "downlevelIteration": true,
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "esModuleInterop": true
  }
}
